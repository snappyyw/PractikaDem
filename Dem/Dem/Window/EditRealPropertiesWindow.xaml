        <TextBox x:Name="textBoxFloor" Height="29" Margin="0,65,47,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Right" Width="143"/>
        <Label x:Name="label_Copy8" Content="Город" HorizontalAlignment="Left" Margin="19,186,0,161" FontSize="16" d:LayoutOverrides="Height"/>
        <Label x:Name="label_Copy9" Content="Номер кв" HorizontalAlignment="Left" Margin="300,188,0,0" FontSize="16" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxСity" Margin="141,188,0,161" TextWrapping="Wrap" HorizontalAlignment="Left" Width="143"/>
        <TextBox x:Name="textBoxApartmentNumber" Margin="385,188,352,160" TextWrapping="Wrap"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Right" Margin="0,123,47,0" VerticalAlignment="Top" Width="143" Height="30"/>


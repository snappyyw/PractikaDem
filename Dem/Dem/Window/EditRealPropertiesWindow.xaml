        <Label x:Name="label_Copy4" Content="Улица" HorizontalAlignment="Left" Margin="300,123,0,0" FontSize="16" VerticalAlignment="Top" d:LayoutOverrides="VerticalAlignment"/>
        <TextBox x:Name="textBoxLongitude" Height="29" Margin="385,14,352,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLatitude" Height="29" Margin="385,65,352,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxStreet" Margin="385,123,352,0" TextWrapping="Wrap" Height="30" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy5" Content="Номер дома" HorizontalAlignment="Right" Margin="0,14,199,0" VerticalAlignment="Top" FontSize="16"/>
        <Label x:Name="label_Copy6" Content="Этаж" HorizontalAlignment="Right" Margin="0,65,199,0" VerticalAlignment="Top" FontSize="16"/>
        <Label x:Name="label_Copy7" Content="Тип" HorizontalAlignment="Right" Margin="0,123,197,0" FontSize="16" VerticalAlignment="Top" d:LayoutOverrides="VerticalAlignment"/>

        <Label x:Name="label_Copy1" Content="Площадь" HorizontalAlignment="Left" Margin="19,123,0,0" FontSize="16" VerticalAlignment="Top" d:LayoutOverrides="VerticalAlignment"/>
        <TextBox x:Name="textBoxNumberOfFloors" HorizontalAlignment="Left" Height="29" Margin="141,16,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143"/>
        <TextBox x:Name="textBoxNumberOfRooms" HorizontalAlignment="Left" Height="29" Margin="141,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143"/>
